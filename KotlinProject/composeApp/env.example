# AI Provider API Keys
GEMINI_API_KEY=your_gemini_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPGRAM_API_KEY=your_deepgram_api_key_here
ELEVEN_LABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Agent Platform Configuration
# Create agents in ElevenLabs Dashboard (https://elevenlabs.io/conversational-ai)
# Each agent is configured with language-specific personality and teaching behavior
ELEVENLABS_ENGLISH_AGENT_ID=your_english_agent_id_here
ELEVENLABS_FRENCH_AGENT_ID=your_french_agent_id_here
ELEVENLABS_GERMAN_AGENT_ID=your_german_agent_id_here
ELEVENLABS_SPANISH_AGENT_ID=your_spanish_agent_id_here
ELEVENLABS_CHINESE_AGENT_ID=your_chinese_agent_id_here
ELEVENLABS_KOREAN_AGENT_ID=your_korean_agent_id_here

# Deepgram Configuration
# Model: Use nova-2 or nova-3 for multilingual code-switching support
DEEPGRAM_MODEL=nova-3
# Language: Use 'multi' for automatic multilingual code-switching
DEEPGRAM_LANGUAGE=multi
# Endpointing: Recommended 100ms for code-switching in streaming
DEEPGRAM_ENDPOINTING=100

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000
ENVIRONMENT=development

# Chat History Configuration
ENABLE_CHAT_HISTORY=true
COMPRESSION_TYPE=gzip
